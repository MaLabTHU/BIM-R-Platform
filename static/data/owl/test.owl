<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/tutorial/pizza#"
     xml:base="http://www.semanticweb.org/tutorial/pizza"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/tutorial/pizza">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A pizza ontology that describles various pizzas based on their toppings</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/tutorial/pizza#hasBase -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tutorial/pizza#hasBase">
        <owl:equivalentProperty>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isBaseOf"/>
            </rdf:Description>
        </owl:equivalentProperty>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasIngredient"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isIngredientOf"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isBaseOf"/>
        <owl:inverseOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasBase"/>
            </rdf:Description>
        </owl:inverseOf>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tutorial/pizza#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#hasCountryOfOrigin -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tutorial/pizza#hasCountryOfOrigin">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasCountryOfOrigin"/>
            </rdf:Description>
        </owl:inverseOf>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#hasIngredient -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tutorial/pizza#hasIngredient">
        <owl:equivalentProperty>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isIngredientOf"/>
            </rdf:Description>
        </owl:equivalentProperty>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <owl:inverseOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasIngredient"/>
            </rdf:Description>
        </owl:inverseOf>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#hasSpiciness -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tutorial/pizza#hasSpiciness">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasSpiciness"/>
            </rdf:Description>
        </owl:inverseOf>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tutorial/pizza#SpicinessValuePartition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#hasTopping -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tutorial/pizza#hasTopping">
        <owl:equivalentProperty>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isToppingOf"/>
            </rdf:Description>
        </owl:equivalentProperty>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasIngredient"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isIngredientOf"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isToppingOf"/>
        <owl:inverseOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
            </rdf:Description>
        </owl:inverseOf>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tutorial/pizza#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#isBaseOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tutorial/pizza#isBaseOf">
        <owl:equivalentProperty>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasBase"/>
            </rdf:Description>
        </owl:equivalentProperty>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isIngredientOf"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasIngredient"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <owl:inverseOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isBaseOf"/>
            </rdf:Description>
        </owl:inverseOf>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tutorial/pizza#PizzaBase"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#isIngredientOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tutorial/pizza#isIngredientOf">
        <owl:equivalentProperty>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasIngredient"/>
            </rdf:Description>
        </owl:equivalentProperty>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasIngredient"/>
        <owl:inverseOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isIngredientOf"/>
            </rdf:Description>
        </owl:inverseOf>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#isToppingOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tutorial/pizza#isToppingOf">
        <owl:equivalentProperty>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
            </rdf:Description>
        </owl:equivalentProperty>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isIngredientOf"/>
        <rdfs:subPropertyOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasIngredient"/>
            </rdf:Description>
        </rdfs:subPropertyOf>
        <owl:inverseOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#isToppingOf"/>
            </rdf:Description>
        </owl:inverseOf>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tutorial/pizza#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->


    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#topObjectProperty">
        <owl:inverseOf>
            <rdf:Description>
                <owl:inverseOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
            </rdf:Description>
        </owl:inverseOf>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/tutorial/pizza#hasCalorificContentValue -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/tutorial/pizza#hasCalorificContentValue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/tutorial/pizza#AmericanHotPizza -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#AmericanHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#JalapenoPepperTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A pizza that only has Mozarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#AmericanaPizza -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#PepperoniTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A pizza that only has Mozarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#AnchovyTopping -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#SeafoodTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#CaperTopping -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#CheeseTopping -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#CheesyPizza -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#Country -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#Country">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#America"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#England"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#France"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#Germany"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#Italy"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#DeepPanBase -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/tutorial/pizza#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#FourCheesePizza -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#FourCheesePizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/tutorial/pizza#CheeseTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#GreenPepperTopping -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#PepperTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#HamTopping -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#HighCaloriePizza -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#Hot -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#Hot">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#SpicinessValuePartition"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#InterestingPizza -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#JalapenoPepperTopping -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasSpiciness"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#LowCaloriePizza -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#LowCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasCalorificContentValue"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#Pizza"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/tutorial/pizza#MargheritaPizza -->


    <owl:Class rdf:about="http://www.semanticweb.org/tutorial/pizza#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/tutorial/pizza#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/tutorial/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/tutorial/pizza#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
